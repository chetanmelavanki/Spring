package com.xworkz.tester;

import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

import com.xworkz.impl.TrainImpl;
import com.xworkz.trainentity.TrainEntity;

public class ServiceTester {
	public static void main(String[] args) {
		ApplicationContext applicationContext = new ClassPathXmlApplicationContext("context-servlet.xml");
		TrainImpl impl = applicationContext.getBean(TrainImpl.class);
		TrainEntity entity = new TrainEntity();
		entity.setTrainName("golgumbaz");
		entity.setTrainColor("Red");
		entity.setTrainSpeed(200);
		entity.setTrainCompartments("100");
	}
}
